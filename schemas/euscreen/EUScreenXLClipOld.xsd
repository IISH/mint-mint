<schema xmlns="http://www.w3.org/2001/XMLSchema" 
		xmlns:sch="http://purl.oclc.org/dsdl/schematron" 			
		xmlns:eus="http://www.euscreen.eu/schemas/euscreen/" 
		targetNamespace="http://www.euscreen.eu/schemas/euscreen/" 
		elementFormDefault="qualified" attributeFormDefault="unqualified"
		sch:queryBinding="xslt2" >
	<element name="EUScreen">
		<complexType>
			<annotation>
				<documentation> The root element declaration </documentation>
				<!-- Declared namespaces for schematron validation -->
				<appinfo>
					<sch:title>Schematron validation</sch:title>
					<sch:ns prefix="eus" uri="http://www.euscreen.eu/schemas/euscreen/" />
				</appinfo>
			</annotation>
			<sequence>
				<element name="metadata" type="eus:MetadataType" maxOccurs="unbounded"/>
			</sequence>
		</complexType>
	</element>
	<complexType name="MetadataType">
		<annotation>
			<appinfo>
				<sch:pattern name="Check metadata">
					<sch:rule context="eus:metadata">
						<sch:assert	test="( (eus:ObjectDescriptiveMetadata/eus:SpatioTemporalInformation/eus:TemporalInformation/eus:broadcastDate[matches(normalize-space(),'\d\d/\d\d/\d\d\d\d')] ) or 
										    (eus:ObjectDescriptiveMetadata/eus:SpatioTemporalInformation/eus:TemporalInformation/eus:productionYear[matches(normalize-space(),'\d\d\d\d')] )
										  )">One of broadcastDate (as dd/mm/yyyy) or productionYear (as YYYY) is required.</sch:assert>
						<sch:assert test="eus:AdministrativeMetadata/eus:identifier[matches(normalize-space(),'EUS_[0-9A-F]{32}')]">There is no valid EUscreen identifier</sch:assert>
					</sch:rule>
				</sch:pattern>
			</appinfo>
		</annotation>
		<sequence>
			<element name="AdministrativeMetadata" type="eus:AdministrativeMetadataType"/>
			<element name="ContentDescriptiveMetadata" type="eus:ContentDescriptiveMetadataType"/>
			<element name="ObjectDescriptiveMetadata" type="eus:ObjectDescriptiveMetadataType"/>
		</sequence>
	</complexType>
	<complexType name="ContentDescriptiveMetadataType">
		<sequence>
			<element name="TitleSet" type="eus:TitleSetType"
				minOccurs="1" maxOccurs="1" />
			<element name="originallanguage" minOccurs="0" maxOccurs="unbounded">
				<complexType>
					<simpleContent>
						<extension base="eus:langType">
							<attribute name="olcode" type="string" />
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="localKeyword" type="string" minOccurs="0"
				maxOccurs="unbounded" />
			<element name="clipTitle" type="string" minOccurs="0" />
			<element name="summary" type="string" />
			<element name="summaryInEnglish" type="string" />
			<element name="ThesaurusTerm" minOccurs="1"
				maxOccurs="unbounded">
				<complexType>
					<simpleContent>
						<extension base="string">
							<attribute name="thestermCode"
								type="string" />
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="genre" minOccurs="1" maxOccurs="unbounded">
				<complexType>
					<simpleContent>
						<extension base="eus:genreType">
							<attribute name="genreCode" type="string" />
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="topic" minOccurs="0" maxOccurs="1">
				<complexType>
					<simpleContent>
						<extension base="eus:topicType">
							<attribute name="topicCode" type="string" />
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="extendedDescription" type="string"
				minOccurs="0" />
			<element name="seriesSeasonNumber" type="string"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="episodeNumber" type="string" maxOccurs="1" minOccurs="0"></element>
		</sequence>
	</complexType>
	<complexType name="ObjectDescriptiveMetadataType">
		<sequence>
			<element name="recordType">
				<complexType>
					<simpleContent>
						<extension base="eus:itemTypeType">
							<attribute name="itemCode" type="string"/>
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="information" type="string" minOccurs="0"/>
			<element name="contributor" type="string" minOccurs="0" maxOccurs="unbounded"/>
			<element name="relationIdentifier" minOccurs="0" maxOccurs="unbounded">
				<complexType>
					<simpleContent>
						<extension base="string">
							<attribute name="relationtype" type="eus:relationTypeType"/>
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="LanguageInformation" type="eus:LanguageInformationType" minOccurs="0" maxOccurs="1"/>
			<element name="SpatioTemporalInformation" type="eus:SpatioTemporalInformationType" minOccurs="1" maxOccurs="1"/>
			<element name="TechnicalInformation" type="eus:TechnicalInformationType" minOccurs="1"/>
		</sequence>
	</complexType>
	<complexType name="AdministrativeMetadataType">
		<sequence>
			<element name="provider">
				<complexType>
					<simpleContent>
						<extension base="eus:providersType">
							<attribute name="providerCode" type="string"/>
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="publisherbroadcaster" type="string" maxOccurs="1" minOccurs="1"/>
