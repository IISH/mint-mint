						</extension>
					</simpleContent>
				</complexType>
			</element>
            <element name="subtitleLanguage" minOccurs="0"
				maxOccurs="unbounded">
				<complexType>
					<simpleContent>
						<extension base="eus:langType">
							<attribute name="slCode" type="string" />
						</extension>
					</simpleContent>
				</complexType>
			</element>
			<element name="metadatalanguage" minOccurs="0">
