							<attribute name="mlCode" type="string" />
						</extension>
					</simpleContent>
				</complexType>
			</element>
		</sequence>
	</complexType>
	<complexType name="SpatialInformationType">
		<sequence>
            <element name="GeographicalCoverage" minOccurs="0" maxOccurs="unbounded">
