<!--
    EADI Schema (XSD 1.1)
    Version: 1.1
    Date: 20140430
    Publisher: Israel Archives Network project
                http://web.nli.org.il/sites/NLI/Hebrew/library/aboutus/now/projects/IAN/
    
    This version of EADI schema has been written from the official 2002 schema to accommodate the needs of the Israel Archives Network project.
    
    Editors:
    Eyal Reuven (The National Library of Israel)
    Yael Gherman (Yad Vashem - The Holocaust Martyrs' and Heroes' Remembrance Authority)
    
    Revision history:
    4) 2014-08-26:
    * Added some more enumerations to "format".
    * Fixed a typo in @type of "file" type (date=>data).
    3) 2014-04-30:
    * Date for Chronitem is not mandatory anymore.
    * Custodian history (custodhist) is now available for all levels.
    * Custodian history now supports the description of previous unitids.
    * Vertex(es) are now encapsulated within a "location" wrapper.
    2) 2014-03-17:
    * Attribute "unit" of type "Extent" field was removed.
    * Attribute "value" of type "Dimensions" is now called "formattedValue".
    * Acqinfo type can now contain the element <p> which can contain the type <persname>, which will be used to specify the owners, if exists.
    * Persname, Corpname and thesaurusElement are now mixed to allow accomodation of values in addition to their attributes.
    * phystech and controlAccess are now referenced from the m.desc.allLevels group.
    * The "Printer" field was added under "imprint".
    * Added digitalCopies file locations for use when importing data.
    * Added repositoryCode regular expression.
    * Date-eadi type now allows filling only unformatted date (element value) to satisfy the mandatory date definition. 
    
    1) 2014-03-06: 
    * EADI Schema draft released.
-->
<xs:schema elementFormDefault="qualified" targetNamespace="urn:isbn:1-931666-22-9"
    xmlns="urn:isbn:1-931666-22-9" xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">  
    <xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="http://www.loc.gov/standards/xlink/xlink.xsd"/>
        
    <!--  ******************************* -->    
    <!--        EAD ROOT element          --> 
    <!--  ******************************* -->

    <xs:element name="ead">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="eadheader" type="eadheader" minOccurs="1" maxOccurs="1" />
                <xs:element name="archdesc" type="archdesc" minOccurs="1" maxOccurs="1" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    
    <!-- ********************************************************* -->
    <!--       XML elements definition (mostly complexTypes)       -->
    <!-- ********************************************************* -->
    
    <xs:simpleType name="boolean">
        <xs:restriction base="xs:NMTOKENS">
            <xs:enumeration value="true"/>
            <xs:enumeration value="false"/>
        </xs:restriction>
    </xs:simpleType>    
    
    <xs:simpleType name="nonEmptyString">
        <xs:restriction base="xs:string">
            <xs:pattern value=".*[^\s].*"/>
        </xs:restriction>
    </xs:simpleType>
    
    <!-- complexType date definition -->
    <xs:attributeGroup name="am.date.normal">
        <xs:attribute name="normal">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:pattern
                        value="(\-?(0|1|2)([0-9]{3})(((01|02|03|04|05|06|07|08|09|10|11|12)((0[1-9])|((1|2)[0-9])|(3[0-1])))|\-((01|02|03|04|05|06|07|08|09|10|11|12)(\-((0[1-9])|((1|2)[0-9])|(3[0-1])))?))?)(/\-?(0|1|2)([0-9]{3})(((01|02|03|04|05|06|07|08|09|10|11|12)((0[1-9])|((1|2)[0-9])|(3[0-1])))|\-((01|02|03|04|05|06|07|08|09|10|11|12)(\-((0[1-9])|((1|2)[0-9])|(3[0-1])))?))?)?"
                    />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="am.dates.calendar">
        <xs:attribute default="gregorian" name="calendar">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="gregorian"/>
                    <xs:enumeration value="Hebrew"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="am.dates.era">
        <xs:attribute default="ce" name="era">            
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="ce"/>
                    <xs:enumeration value="bce"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:complexType mixed="true" name="date-eadi">
        <xs:attributeGroup ref="am.dates.era"/>
        <xs:attributeGroup ref="am.dates.calendar"/>
        <xs:attributeGroup ref="am.date.normal"/>
        <xs:attribute name="certainty"/>
        <xs:attribute name="encodinganalog"/>
        
    </xs:complexType>

    <xs:complexType name="imprint"><!-- דפוסת -->
        <xs:all>
            <xs:element name="geogname" type="xs:string" minOccurs="0" maxOccurs="1"/><!-- מקום הוצאה לאור -->
            <xs:element name="date" type="date-eadi" minOccurs="0" maxOccurs="1"/> <!-- תאריך הוצאה לאור -->
            <xs:element name="publisher" type="publisher" minOccurs="0" maxOccurs="1"/>
            <xs:element name="printer" type="xs:string" minOccurs="0" maxOccurs="1"/><!-- דפוס -->
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="origination">        
        <xs:group ref="corpnameAndPersnameBundle" minOccurs="1" maxOccurs="unbounded"/>
    </xs:complexType>
    
    <xs:group name="corpnameAndPersnameBundle">
        <xs:choice>
            <xs:element name="corpname" type="corpname" maxOccurs="unbounded" />
            <xs:element name="persname" type="persname" maxOccurs="unbounded" />            
        </xs:choice>
    </xs:group>
    
    <xs:complexType name="thesaurusElement" mixed="true">
        <xs:attributeGroup ref="thesaurusAccess"/>
    </xs:complexType>
        
    <xs:complexType name="corpname" mixed="true">
        <xs:attribute name="role" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="creator"/>
                    <xs:enumeration value="initiatorStudio"/>
                    <xs:enumeration value="PhotographingStudio"/>
                    <xs:enumeration value="DevelopingStudio"/>
                    <xs:enumeration value="publisher"/>
                </xs:restriction>
            </xs:simpleType>            
        </xs:attribute>
        <xs:attributeGroup ref="thesaurusAccess"/>
    </xs:complexType>
    
    <xs:complexType name="persname" mixed="true">
        <xs:attribute name="role" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="photographer"/>
                    <xs:enumeration value="creator"/>
                    <xs:enumeration value="publisher"/>
                    <xs:enumeration value="typist"/>
                    <xs:enumeration value="proofreader"/>
                    <xs:enumeration value="cataloguer"/>
                    <xs:enumeration value="architect"/>
                    <xs:enumeration value="interviewer"/>
                    <xs:enumeration value="producer"/>
                    <xs:enumeration value="editor"/>
                    <xs:enumeration value="director"/>
                    <xs:enumeration value="leadingActor"/>                    
                    <xs:enumeration value="owners"/>                    
                </xs:restriction>
            </xs:simpleType>            
        </xs:attribute>
        <xs:attributeGroup ref="thesaurusAccess"/>
    </xs:complexType>
    
    <xs:complexType name="unittitle" mixed="true" >
        <xs:all>
            <xs:element name="imprint" type="imprint" minOccurs="0" maxOccurs="1" /><!-- דפוסת -->
            <xs:element name="title" type="nonEmptyString" minOccurs="1" maxOccurs="1"/><!-- כותר -->
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="period" mixed="true"><!-- תקופה/תקופות -->
        <xs:attributeGroup ref="thesaurusAccess"/>
    </xs:complexType>
    
    <xs:complexType name="scopeContent" mixed="true"/><!-- תוכן -->
            
    <xs:complexType name="extent" mixed="true"/><!-- היקף -->

    <xs:complexType name="medium" mixed="true">
        <xs:attribute name="mediumType" use="required"><!-- מדיה -->
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Paper"/>
                    <xs:enumeration value="Photograph"/>
                    <xs:enumeration value="Still Film Roll"/>
                    <xs:enumeration value="Electronic Resource"/>
                    <xs:enumeration value="Gramophone Record"/>
                    <xs:enumeration value="Video Tape"/>
                    <xs:enumeration value="Compact Disc"/>
                    <xs:enumeration value="Magnetic Film"/>
                    <xs:enumeration value="Floppy Disk"/>
                    <xs:enumeration value="External Hard Drive"/>
                    <xs:enumeration value="Film Motion Picture Roll"/>
                    <xs:enumeration value="Black and White Motion Picture Roll"/>
                    <xs:enumeration value="Reel-to-reel Audio Tape Recording"/>
                    <xs:enumeration value="Electronic Resource"/>
                    <xs:enumeration value="Wax cylinder"/>
                    <xs:enumeration value="Parchment"/>                    
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType> 
        </xs:attribute>        
        <xs:attribute name="format"><!-- פורמט/פורמטים -->
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="TIFF"/>
                    <xs:enumeration value="JPEG"/>
                    <xs:enumeration value="BMP"/>
                    <xs:enumeration value="MP3"/>
                    <xs:enumeration value="WAVE"/>
                    <xs:enumeration value="PDF"/>
                    <xs:enumeration value="MDB"/>
                    <xs:enumeration value="EXCEL"/>
                    <xs:enumeration value="WORD"/>
                    <xs:enumeration value="HTML"/>
                    <xs:enumeration value="POWERPOINT"/>
                    <xs:enumeration value="A0"/>
                    <xs:enumeration value="A1"/>
                    <xs:enumeration value="A2"/>
                    <xs:enumeration value="A3"/>
                    <xs:enumeration value="A4"/>
                    <xs:enumeration value="A5"/>
                    <xs:enumeration value="Film Negative"/>
                    <xs:enumeration value="Film Positive"/>
                    <xs:enumeration value="Silver Print"/>
                    <xs:enumeration value="Albumen Print"/>
                    <xs:enumeration value="Glass Negative"/>
                    <xs:enumeration value="VHS"/>                  
                    <xs:enumeration value="Mini-DV"/>
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType> 
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="diplomaticsForm"><!--  צורה דיפלומטית -->
        <xs:attribute name="type">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Incoming/Outgoing Telegram"/>
                    <xs:enumeration value="Letter"/>
                    <xs:enumeration value="Official letter"/>
                    <xs:enumeration value="Protocol"/>
                    <xs:enumeration value="Report"/>
                    <xs:enumeration value="Contract"/>
                    <xs:enumeration value="Speech"/>
                    <xs:enumeration value="Article"/>
                    <xs:enumeration value="Notice"/>
                    <xs:enumeration value="Minutes"/>
                    <xs:enumeration value="Postcard"/>
                    <xs:enumeration value="Announcement"/>
                    <xs:enumeration value="Decleration"/>
                    <xs:enumeration value="Arbitration Decision"/>
                    <xs:enumeration value="List"/>
                    <xs:enumeration value="Diary"/>
                    <xs:enumeration value="Note"/>
                    <xs:enumeration value="Poem"/>
                    <xs:enumeration value="Interview"/>
                    <xs:enumeration value="Photograph"/>
                    <xs:enumeration value="Map"/>
                    <xs:enumeration value="Poster"/>
                    <xs:enumeration value="History"/>
                    <xs:enumeration value="Schematics"/>
                    <xs:enumeration value="Motion Picture"/>
                    <xs:enumeration value="Coin/Medal"/>
                    <xs:enumeration value="Exhibit"/>
                    <xs:enumeration value="Memorandum"/>
                    <xs:enumeration value="Notice"/>
                    <xs:enumeration value="Advertising flyer"/>
                    <xs:enumeration value="Brochure"/>
                    <xs:enumeration value="Program"/>
                    <xs:enumeration value="Ticket"/>
                    <xs:enumeration value="Invitation"/>                    
                    <xs:enumeration value="Sticker"/>                    
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="accessrestrict" mixed="true"><!-- חסוי בפני הצגה באינטרנט או הרשאה להצגה-->
        <xs:attribute name="digitalObjectAccess" type="boolean" default="true" />
        <xs:attribute name="metadataAccess" type="boolean" default="true" />
    </xs:complexType>
    
    <xs:complexType name="userestrict" mixed="true"/><!--  תנאי העתקה / זכויות יוצרים -->
    
    <xs:complexType name="phystech">
        <xs:sequence>
            <xs:element name="physicalCondition" type="physicalCondition" minOccurs="1" maxOccurs="unbounded" /><!-- מצב פיזי -->
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="originalsLoc" mixed="true"/><!-- מיקום חומר מקור -->
    
    <xs:complexType name="physloc" mixed="true"/><!-- מיקום פיזי / מצב ההשאלה של הפריט -->
    
    <xs:complexType name="note" mixed="true"/>
    
    <xs:complexType name="otherfindaid" mixed="true"/>
    
    <xs:complexType name="descriptionUnitStatus">
        <xs:attribute name="status"><!-- מצב העבודה על יחידת התיאור -->
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Unverified Draft"/>
                    <xs:enumeration value="Edited Draft"/>
                    <xs:enumeration value="Final"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
       
    <xs:complexType name="language">
        <xs:attributeGroup ref="langcode"/>
        <xs:attributeGroup ref="scriptcode"/>
    </xs:complexType>
    
    <xs:complexType name="langmaterial">
        <xs:sequence>
            <xs:element name="language" type="language" minOccurs="0" maxOccurs="unbounded" /><!-- שפה -->
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="bibliography" mixed="true"/>
    
    <xs:complexType name="bioghist" mixed="true"/>

    <xs:complexType name="custodhist" mixed="true">
        <xs:sequence>
            <xs:element name="originalunitid" type="originalunitid" minOccurs="0" maxOccurs="unbounded"/><!-- סימול/מזהה מקורי -->
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="appraisal" mixed="true"/>
    
    <xs:complexType name="accruals" mixed="true"/>
    
    <xs:complexType name="arrangement" mixed="true"/>
    
    <xs:complexType name="processingStatus" mixed="true"/>
    
    <xs:complexType name="acqinfo">
        <xs:sequence>
            <xs:element name="p">
                <xs:complexType mixed="true">
                    <xs:sequence>
                        <xs:element name="persname" type="persname" minOccurs="0" maxOccurs="unbounded"/><!-- בעלים -->
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="genreform">
        <xs:attribute name="type"><!-- סוג חומר -->
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Audio"/>
                    <xs:enumeration value="Videos"/>
                    <xs:enumeration value="Posters"/>
                    <xs:enumeration value="Schamatics"/>
                    <xs:enumeration value="Oral History"/>
                    <xs:enumeration value="Photographs"/>
                    <xs:enumeration value="Objects"/>
                    <xs:enumeration value="Documents"/>
                    <xs:enumeration value="Maps"/>                    
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="dimensions">
        <xs:attribute name="type">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Width"/>
                    <xs:enumeration value="Height"/>
                    <xs:enumeration value="Depth"/>
                    <xs:enumeration value="Duration"/>
                    <xs:enumeration value="Weight"/>
                    <xs:enumeration value="Length"/>
                    <xs:enumeration value="Other"/>                    
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="unit" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Seconds"/>
                    <xs:enumeration value="Minutes"/>
                    <xs:enumeration value="Hours"/>
                    <xs:enumeration value="Days"/>
                    <xs:enumeration value="Pages"/>
                    <xs:enumeration value="Kilograms"/>
                    <xs:enumeration value="Grams"/>
                    <xs:enumeration value="Centimeters"/>
                    <xs:enumeration value="Meters"/>
                    <xs:enumeration value="Kilometers"/>
                    <xs:enumeration value="Inch (US)"/>
                    <xs:enumeration value="Foot (US)"/>
                    <xs:enumeration value="Yard (US)"/>
                    <xs:enumeration value="Mile (US)"/>
                    <xs:enumeration value="Inch (Imperial)"/>
                    <xs:enumeration value="Foot (Imperial)"/>
                    <xs:enumeration value="Mile (Imperial)"/>
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType>            
        </xs:attribute>
        <xs:attribute name="formattedValue" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:pattern value="[0-9]*" />
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="publisher">
        <xs:group ref="corpnameAndPersnameBundle" minOccurs="0" maxOccurs="1" /> <!-- שם מוציא לאור -->        
    </xs:complexType>

    <xs:complexType name="color">
        <xs:attribute name="value">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Black and white"/>
                    <xs:enumeration value="Brown"/>
                    <xs:enumeration value="Color"/>
                    <xs:enumeration value="Shaded"/>
                    <xs:enumeration value="Hand-drawn"/>
                    <xs:enumeration value="Other"/>
                </xs:restriction>
            </xs:simpleType>            
        </xs:attribute>        
    </xs:complexType>

