    <!--             descriptive elements            -->
    <!--  ****************************************** -->
        
    <xs:complexType name="did">
      <xs:all>
          <xs:element name="origination" type="origination" minOccurs="1" maxOccurs="1"/><!-- חובה --><!-- יוצר--><!-- שם הסטודיו--><!-- שם המהנדס/האדריכל --><!-- שם המראיין --><!-- מפיק--><!-- עורך -->
          <xs:element name="unitdate" type="date-eadi" minOccurs="1" maxOccurs="1" /><!-- חובה --><!-- תאריך/תאריכים --><!-- תאריך הפקה -->
          <xs:element name="unitid" type="nonEmptyString" minOccurs="1" maxOccurs="1"/><!-- סימול מזהה--><!-- חובה -->
          <xs:element name="unittitle" type="unittitle" minOccurs="1" maxOccurs="1" /><!-- שם/כותר -->
          <xs:element name="period" type="period" minOccurs="0" maxOccurs="1" /><!-- תקופה/תקופות --><!-- תקופת אירועים -->
          <xs:element name="diplomaticsForm" type="diplomaticsForm" minOccurs="0" maxOccurs="1"/><!--  צורה דיפלומטית -->
          <xs:element name="physloc" type="physloc" minOccurs="0" maxOccurs="1"/><!--  מיקום פיזי / מצב ההשאלה של הפריט -->
          <xs:element name="physdesc" type="physdesc" minOccurs="1" maxOccurs="1"/>          
          <xs:element name="langmaterial" type="langmaterial" minOccurs="0" maxOccurs="1"/>
      </xs:all>
