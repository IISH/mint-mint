    <xs:complexType name="c.subfondlevel">
        <xs:sequence>
            <xs:group ref="m.desc.allLevels" minOccurs="1" maxOccurs="1"/>
            <xs:group ref="m.desc.highLevels" minOccurs="1" maxOccurs="1"/>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
