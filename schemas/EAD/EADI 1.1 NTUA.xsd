          <xs:element name="chronlist" type="chronlist"/>
          <xs:element name="descriptionUnitStatus" type="descriptionUnitStatus" minOccurs="0" maxOccurs="1"/>
          <xs:element name="note" type="processingStatus" minOccurs="0" maxOccurs="1"/><!-- מצב סידור/רישום -->
      </xs:all>
    </xs:complexType>
       
    <xs:complexType name="chronlist">
      <xs:all>
          <xs:element name="chronitem" type="chronitem" minOccurs="1" maxOccurs="1"/>
      </xs:all>
    </xs:complexType>
    
    <xs:complexType name="chronitem">
        <xs:all>
            <xs:element name="date" type="date-eadi" minOccurs="0" maxOccurs="1"/><!-- תאריך קיטלוג -->
            <xs:element name="persname" type="persname" minOccurs="0" maxOccurs="1"/>
        </xs:all>
    </xs:complexType>
        
    <xs:complexType name="physdesc">
        <xs:all>
            <xs:element name="extent" type="extent" minOccurs="1" maxOccurs="1"/><!-- היקף --><!-- חובה -->
            <xs:element name="medium" type="medium" minOccurs="0" maxOccurs="1"/>
            <xs:element name="genreform" type="genreform" minOccurs="0" maxOccurs="1"/>
            <xs:element name="dimensions" type="dimensions" minOccurs="0" maxOccurs="1"/><!-- מספר דפים/עמודים --><!-- משך הראיון --><!-- גודל כרזה --><!-- גודל תצלום --><!-- גודל מפה --><!-- משך --><!-- ממדי החפץ -->
            <xs:element name="physfacet" type="physfacet" minOccurs="0" maxOccurs="1"/>            
        </xs:all>
    </xs:complexType>
    
    <!--  ******************************************************* -->
    <!--      groups of hierarchic descriptive sub-elements       -->
    <!--  ******************************************************* -->

    <xs:group name="m.desc.lowLevels">
        <xs:sequence>
            <xs:element name="genreSpecificData" type="genreSpecificData" minOccurs="0" maxOccurs="1"/>
            <xs:element name="geographicalLocation" type="geographicalLocation" minOccurs="0" maxOccurs="unbounded"/><!-- מיקום גיאוגרפי -->
            <xs:element name="digitalCopies" type="digitalCopies" minOccurs="0" maxOccurs="1">
                <xs:unique name="filenames_uniqueness">
                    <xs:selector xpath="file"/>
                    <xs:field xpath="@path"/>
                </xs:unique>
            </xs:element>            
        </xs:sequence>
    </xs:group>

    <xs:group name="m.desc.highLevels">
        <xs:sequence>
            <xs:element name="bioghist" type="bioghist" minOccurs="0" maxOccurs="1"/><!-- היסטוריה מנהלית/ביוגרפיה -->
            <xs:element name="appraisal" type="appraisal" minOccurs="0" maxOccurs="1"/><!-- מידע על הערכה, ביעור ותקופות אחזקה -->
            <xs:element name="accruals" type="accruals" minOccurs="0" maxOccurs="1"/><!-- צפי לרכישות עתידיות-->
            <xs:element name="arrangement" type="arrangement" minOccurs="0" maxOccurs="1"/><!-- שיטת הסידור-->
            <xs:element name="acqinfo" type="acqinfo" minOccurs="0" maxOccurs="unbounded"/><!-- מקור רכישה-->            
        </xs:sequence>
    </xs:group>
    
    <xs:group name="m.desc.allLevels">
        <xs:sequence>
            <xs:element name="did" type="did" minOccurs="1" maxOccurs="1" >
                <xs:unique name="recordId_uniqueness">
                    <xs:selector xpath="did"/>
                    <xs:field xpath="unitid"/>
                </xs:unique>
            </xs:element>
            <xs:element name="processinfo" type="processinfo" minOccurs="0" maxOccurs="1"/><!-- מצב העבודה של הרשומה -->
            <xs:element name="scopecontent" type="scopeContent" minOccurs="0" maxOccurs="1"/><!-- תוכן -->
            <xs:element name="accessrestrict" type="accessrestrict" minOccurs="0" maxOccurs="1" /><!-- תנאי גישה/שימוש -->
            <xs:element name="userestrict" type="userestrict" minOccurs="0" maxOccurs="unbounded" /><!-- תנאי העתקה / זכויות יוצרים-->
            <xs:element name="originalsLoc" type="originalsLoc" minOccurs="0" maxOccurs="unbounded"/><!-- מיקום חומר מקור -->
            <xs:element name="controlaccess" type="controlAccess" minOccurs="0" maxOccurs="1" />
            <xs:element name="phystech" type="phystech" minOccurs="0" maxOccurs="1"/>
            <xs:element name="relatedmaterial" type="relatedmaterial" minOccurs="0" maxOccurs="unbounded"/><!-- קישור לפריטים שייכים -->
            <xs:element name="note" type="note" minOccurs="0" maxOccurs="unbounded"/><!--  הערה -->
            <xs:element name="otherfindaid" type="otherfindaid" minOccurs="0" maxOccurs="unbounded"/><!--  עזרי איתור -->
            <xs:element name="bibliography" type="bibliography" minOccurs="0" maxOccurs="unbounded"/><!-- הפניות -->                      
            <xs:element name="custodhist" type="custodhist" minOccurs="0" maxOccurs="1"/><!-- היסטוריה ארכיונית ומקור הרכישה -->
        </xs:sequence>
    </xs:group>
</xs:schema>
