            <xs:element name="eadid" type="eadid" minOccurs="1" maxOccurs="1"/>
            <xs:element name="revisiondesc" type="revisiondesc" minOccurs="0" maxOccurs="1" />
            <xs:element name="profiledesc" type="profiledesc" minOccurs="0" maxOccurs="1" />
            <xs:element name="filedesc" type="xs:string" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attribute default="iso639-2b" name="langencoding" type="xs:NMTOKEN"/>
        <xs:attribute default="iso15924" name="scriptencoding" type="xs:NMTOKEN"/>
        <xs:attribute default="iso8601" name="dateencoding" type="xs:NMTOKEN"/>
        <xs:attribute default="iso3166-1" name="countryencoding" type="xs:NMTOKEN"/>
        <xs:attribute default="iso15511" name="repositoryencoding" type="xs:NMTOKEN"/>
        <xs:attribute name="relatedencoding" type="xs:NMTOKEN"/>    
    </xs:complexType>    


    <xs:complexType name="eadid">
        <xs:attributeGroup ref="repositoryCode"/><!-- Archive int'l repository code according to ISO 15511 (ISIL) -->
        <xs:attributeGroup ref="countryCode"/>
        <xs:attribute name="identifier" type="xs:string" default="EADI 1.0"/>
    </xs:complexType>
    
    <xs:complexType name="profiledesc">
        <xs:all>
            <xs:element name="creation" type="creation" minOccurs="1" maxOccurs="1"/>
            <xs:element name="descrules" type="descrules" minOccurs="0" maxOccurs="1"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="creation">
        <xs:sequence>
            <xs:element name="date" type="date-eadi" minOccurs="1" maxOccurs="1" /><!--  תאריך יצירה --> 
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="revisiondesc">
        <xs:sequence>
            <xs:element name="change" type="change"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="change">
        <xs:sequence>
            <xs:element name="date" type="date-eadi" minOccurs="1" maxOccurs="1" /><!--  תאריך עדכון -->
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="controlAccess">
        <xs:all>
            <xs:element name="subject" type="thesaurusElement" minOccurs="0" maxOccurs="1"/><!-- מילות מפתח --><!-- נושאים עיקריים -->
            <xs:element name="corpname" type="corpname" minOccurs="0" maxOccurs="1"/>
            <xs:element name="persname" type="persname" minOccurs="0" maxOccurs="1"/>
            <xs:element name="name" type="thesaurusElement" minOccurs="0" maxOccurs="1"/><!-- מכותבים/משתתפים -->
            <xs:element name="geogname" type="thesaurusElement" minOccurs="0" maxOccurs="1" /><!-- מקום -->
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="relatedmaterial">
        <xs:sequence>
            <xs:element name="unitid" type="nonEmptyString"/>
            <xs:element name="unittitle" type="unittitle"/>
            <xs:element name="head" type="xs:string"/><!-- תיאור הקשר -->
            <xs:element name="p" type="xs:string"/><!-- תיאור הקשר -->
        </xs:sequence>
        <xs:attributeGroup ref="repositoryCode"/>
    </xs:complexType>
    
    <xs:complexType name="dsc">
        <xs:choice>
            <xs:element name="c" minOccurs="1" maxOccurs="unbounded">
                </xs:element>                
        </xs:choice>
    </xs:complexType>
    
    <xs:complexType name="digitalCopies">
        <xs:sequence>
            <xs:element name="file" type="file" minOccurs="1" maxOccurs="unbounded">

            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="file">
        <xs:attribute name="path" type="xs:string"/><!-- קובץ עותק דיגיטלי -->
        <xs:attribute name="type">
            <xs:simpleType>
                <xs:restriction base="xs:token">
                    <xs:enumeration value="image"/>
                    <xs:enumeration value="dataFile"/><!-- e.g. a OCR data file -->
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

    <!--  ****************************************** -->
    <!--       hierarchic descriptive elements       -->
    <!--  ****************************************** -->
    
    <xs:complexType name="archdesc">
      <xs:sequence>
